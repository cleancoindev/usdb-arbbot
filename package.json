{
  "name": "arbbot",
  "version": "1.0.0",
  "main": "./build/index.js",
  "scripts": {
    "start-dist": "rm -rf dist && npm run dist && node ./dist/index.js",
    "start-build": "rm -rf dist && npm run build && node ./build/index.js",
    "babel-node": "babel-node --",
    "build": "./node_modules/.bin/babel src -d build",
    "dist": "./node_modules/.bin/babel src -d dist --presets=babili",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register --reporter spec"
  },
  "dependencies": {
    "dotenv": "^6.1.0",
    "jsonfile": "^5.0.0",
    "node-telegram-bot-api": "^0.30.0",
    "solc": "^0.8.13",
    "stringstream": "0.0.6",
    "truffle-hdwallet-provider": "0.0.6",
    "web3": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node": "^6.1.1",
    "babili": "^0.1.3",
    "eslint": "^4.0.0",
    "mocha": "^5.2.0",
    "power-assert": "^1.4.4"
  }
}
